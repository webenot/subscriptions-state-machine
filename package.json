{
  "name": "subscriptions-state-machine",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": false,
  "license": "MIT",
  "scripts": {
    "init": "bash scripts/app-init.sh",
    "prebuild": "rimraf build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "repl": "npm run start -- --entryFile repl",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/main",
    "lint": "eslint \"{src,common,aws-lambdas}/**/*.ts\" --quiet --cache --cache-location=./.eslintcache/.eslintcache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli-ts-node-commonjs.js",
    "typeorm:generate": "bash scripts/postgresql/migration-generate.sh",
    "typeorm:create": "bash scripts/postgresql/migration-create.sh",
    "typeorm:run": "bash scripts/postgresql/migration-run.sh",
    "typeorm:revert": "bash scripts/postgresql/migration-revert.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.677.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "10.0.2",
    "@ssut/nestjs-sqs": "^3.0.0",
    "bignumber.js": "^9.1.2",
    "cache-manager": "^6.1.1",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "nestjs-typeorm-paginate": "^4.0.4",
    "pg": "^8.13.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^17.2.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^3.21.2",
    "@faker-js/faker": "^9.0.3",
    "@nestjs/schematics": "^10.2.2",
    "@nestjs/testing": "^10.4.6",
    "@types/cache-manager-ioredis": "^2.0.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.7.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vtex": "^2.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
